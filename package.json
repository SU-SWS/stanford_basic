{
  "name": "stanford_basic",
  "version": "1.0.0",
  "repository": "https://github.com/SU-SWS/stanford_basic",
  "config": {
    "srcSass": "src/scss",
    "distSass": "dist/css",
    "srcJS": "src/js",
    "distJS": "dist/js",
    "srcAssets": "src/assets",
    "distAssets": "dist/assets"
  },
  "scripts": {
    "build": "webpack --mode=development --display-error-details",
    "publish": "webpack --mode=production",
    "watch": "webpack --mode=development --watch",
    "lint:js": "eslint -c .eslintrc 'src/js'",
    "lint:sass": "sass-lint -c .sass-lint.yml '**/*.scss' -v -q -i 'vendor/**/*.scss, node_modules/**/*.scss'"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "acorn": "^6.3.0",
    "autoprefixer": "^9.6.5",
    "babel-loader": "^8.0.6",
    "bourbon": "^5.1.0",
    "css-loader": "^2.1.1",
    "decanter": "github:su-sws/decanter#v6",
    "eslint": "^5.15.3",
    "extra-watch-webpack-plugin": "^1.0.3",
    "file-loader": "^3.0.1",
    "filemanager-webpack-plugin": "^2.0.5",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "resolve-url-loader": "^3.1.0",
    "sass-lint": "^1.13.1",
    "sass-loader": "^7.3.1",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.41.2",
    "webpack-assets-manifest": "^3.1.1",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2",
    "webpack-fix-style-only-entries": "^0.2.2",
    "webpack-manifest-plugin": "^2.2.0"
  },
  "description": "=========================== How to Use Grunt with Stanford Basic ===========================",
  "main": "Gruntfile.js",
  "author": "",
  "license": "MIT",
  "keywords": [
    "css",
    "scss",
    "Drupal",
    "Stanford"
  ],
  "dependencies": {
    "drupal-behaviors-loader": "^1.0.1",
    "npm": "^5.10.0"
  }
}
